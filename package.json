{
  "name": "@fkn/lib",
  "version": "0.0.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@apollo/client": "^3.3.16",
    "@types/webtorrent": "^0.109.3",
    "assert": "^2.0.0",
    "browserify-fs": "^1.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^6.3.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.15",
    "esbuild-plugin-alias": "^0.2.0",
    "events": "^3.3.0",
    "path": "^0.12.7",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.4.3",
    "url": "^0.11.0",
    "util": "^0.12.4"
  },
  "scripts": {
    "build-index": "node ./build.js",
    "build-index-watch": "node ./build.js -w",
    "build2": "rollup -c",
    "dev2": "rollup -c -w",
    "dev3": "tsc --watch",
    "type-check": "tsc --incremental",
    "type-check-watch": "tsc --watch --incremental",
    "build3": "parcel build ./src/index.ts ./src/service-worker/index.ts && tsc",
    "build": "esbuild ./src/index.ts --format=esm --bundle --outfile=lib/index.js && npm run type-check",
    "build-watch": "esbuild --watch ./src/index.ts --format=esm --bundle --outfile=lib/index.js",
    "dev": "concurrently \"npm run build-watch\" \"npm run type-check-watch\""
  },
  "dependencies": {
    "@mfkn/fkn-web": "file:../web",
    "osra": "^0.0.5"
  }
}
