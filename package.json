{
  "name": "@banou26/oz-lib",
  "version": "0.0.1",
  "main": "lib/lib/src/index.js",
  "license": "MIT",
  "devDependencies": {
    "@apollo/client": "^3.3.16",
    "@rollup/plugin-typescript": "^8.2.1",
    "parcel": "^2.0.0-nightly.653",
    "rollup": "^2.46.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "build2": "rollup -c",
    "dev2": "rollup -c -w",
    "dev": "tsc --watch",
    "build": "parcel build ./src/index.ts && tsc"
  },
  "lib": "dist/index.js",
  "targets": {
    "lib": {
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": ["@mfkn/oz-web"]
    }
  },
  "alias": {
    "@mfkn/oz-web": "../web"
  }
}
